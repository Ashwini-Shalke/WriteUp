# platform :iOS, '9.0'


def shared_pods 
 pod 'FSCalendar'
 pod 'Firebase/Core'
 pod 'Firebase/Crashlytics'
 pod 'Firebase/Auth'
 pod 'Firebase/Database'
 pod 'Firebase/Firestore'
 pod 'FirebaseFirestoreSwift'
end


target 'WriteUp' do
  use_frameworks!
    shared_pods
end

target 'WriteUpDev' do
  use_frameworks!
    shared_pods
end
